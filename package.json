    "@angular/platform-browser-dynamic": "^6.0.5",
    "@angular/router": "^6.0.5",
    "core-js": "^2.4.1",
    "moment": "^2.22.1",
    "moment-es6": "^1.0.0",
    "moment-timezone": "^0.5.17",
    "moment-timezone-es6": "^1.0.0",
    "rxjs": "^6.2.1",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
